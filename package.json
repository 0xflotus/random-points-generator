    "@turf/helpers": "^6.0.1",
    "@turf/points-within-polygon": "^5.1.5",
    "@turf/random": "^5.1.5",
    "@turf/unkink-polygon": "^5.1.5",
    "lodash": "^4.2.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^7.0.0",
    "mocha": "^3.2.0",
    "prettier": "^1.11.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
